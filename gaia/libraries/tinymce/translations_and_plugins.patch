diff -rNuB ./jscripts/tiny_mce/plugins/imagecaption/editor_plugin.js jscripts/tiny_mce/plugins/imagecaption/editor_plugin.js
--- jscripts/tiny_mce/plugins/imagecaption/editor_plugin.js	1970-01-01 01:00:00.000000000 +0100
+++ jscripts/tiny_mce/plugins/imagecaption/editor_plugin.js	2012-04-23 13:40:33.598981595 +0200
@@ -0,0 +1,62 @@
+
+(function() {
+tinymce.create('tinymce.plugins.tinymce_image_caption', {
+  // this function is run when reading of plugin
+  init : function(ed, url) {
+  ed.addCommand('mceAlert', function(vars) {
+    alert(vars);
+  });
+    // creation function resposinble for create Image Caption
+    ed.addCommand('mceCreateCaption', function() {
+
+      var html_caption;
+      var html_img;
+
+      $selected = $('<div/>').append(ed.selection.getContent());
+
+      var $collection = $selected
+        .textNodes()
+        .filter(function() {
+          return this.nodeType == Node.TEXT_NODE;
+        });
+
+      // make caption, author, rest
+      var tmpArray = new Array();
+      $collection.each(function() {
+        var tmpString;
+        if (tmpArray.length == 0)
+          tmpString = 'image-caption';
+        else if (tmpArray.length == 1)
+          tmpString = 'image-author';
+        else
+          tmpString = 'image-rest';
+        tmpArray.push('<span class="' + tmpString + '">' + this.textContent + '</span>');
+      });
+      html_caption = tmpArray.join('');
+
+      var tmpImage = $selected.find('img').appendTo('body');
+      var tmpWidth = tmpImage.width();
+      html_img = $('<div/>').append(tmpImage).html();
+
+      var html = '<p>\n\
+                    <span style="width: ' + tmpWidth + 'px;" class="image-caption-container">\n\
+                      ' + html_img + '\n\
+                      ' + html_caption + '\n\
+                    </span>\n\
+                  </p>';
+
+      ed.execCommand('mceReplaceContent',false,html);
+    });
+
+    // create button in editor
+    ed.addButton('imagecaption', {
+      title : 'Zdjęcie, podpis, autor',
+      cmd : 'mceCreateCaption', // method called when button pressed
+      image : url + '/img/imagecaption.png' // path to image button
+    });
+  }// end init
+}); //end tinymce.create()
+
+// Register plugin
+tinymce.PluginManager.add('tinymce_image_caption', tinymce.plugins.tinymce_image_caption);
+})();
